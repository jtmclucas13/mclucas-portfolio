            "git add"
        ]
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/gatsbyjs/gatsby-starter-default"
    },
    "devDependencies": {
        "babel-eslint": "^10.0.1",
        "eslint-config-prettier": "^4.0.0",
        "eslint-plugin-prettier": "^3.0.1",
        "husky": "^1.3.1",
