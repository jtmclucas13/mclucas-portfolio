        "gatsby-source-filesystem": "^2.0.20",
        "gatsby-transformer-sharp": "^2.1.13",
        "node-sass": "^4.11.0",
        "prop-types": "^15.7.1",
        "react": "^16.8.0",
        "react-breakpoints": "^3.0.3",
        "react-dom": "^16.8.0",
        "react-hamburger-menu": "^1.1.1",
